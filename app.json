{
  "expo": {
    "name": "OneRetire",
    "slug": "unpensionappnativewind",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "unjspf-pension",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.oneretire.app",
      "buildNumber": "1",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "c3d10625-1b38-4c6e-8d61-f437296007aa"
      }
    },
    "android": {
      "package": "com.oneretire.app",
      "versionCode": 2,
      "adaptiveIcon": {
        "foregroundImage": "https://res.cloudinary.com/djd2pcr44/image/upload/v1758718205/ChatGPT_Image_Sep_24_2025_06_11_37_PM_uo4doa.png",
        "backgroundColor": "#ffffff"
      }
    }
  }
}
